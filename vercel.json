{
  "version": 2,
  "builds": [
    {
      "src": "dist/index.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/generateChart/(.*)",
      "destination": "/generateChart"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    },
    {
      "source": "/chartImage/(.*)",
      "destination": "/chartImage"
    }
  ]
}