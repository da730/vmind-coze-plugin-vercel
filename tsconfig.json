{
    "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        "rootDir": "src",
        "outDir": "dist",
        "strict": false,
        "moduleResolution": "node",
        "esModuleInterop": true,
        "strictNullChecks": false,
        "noEmitOnError": true,
        "skipLibCheck": true

    },
    "include": ["src/**/*.ts"],
    "exclude": ["node_modules"]
}
